{{>navbarAuthenticated}}
<h2>Create bucketlist for {{username}}</h2>
<form action="/createBucketlist/{{username}}" method="post">
    <input type="hidden" name="_csrf" value="{{csrfToken}}">
    {{>validationSummary}}
    <input type="text" name="title" placeholder="Name your bucketlist..." value="{{title}}" required="required"></input>
    <button class="form-button" type="submit">Create</button>
</form>
